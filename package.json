{
  "name": "edit-tool",
  "version": "1.0.0",
  "description": "A simple edit tool",
  "main": "main.js",
  "scripts": {
    "electron": "electron .",
    "web": "react-app-rewired start",
    "start": "concurrently \"cross-env BROWSER=none PORT=3000 npm run web\" \"wait-on http://localhost:3000 && nodemon --watch main.js --exec npm run electron\"",
    "build": "react-app-rewired build",
    "dist": "npm-run-all dist:*",
    "packagemac": "npm run build && npm run distmac",
    "package": "npm run build && npm run dist",
    "release": "npm run build && npm run upgrade && npm run dist && npm run deploy"
  },
  "dependencies": {
    "antd": "^4.9.4",
    "copy-to-clipboard": "^3.3.1",
    "moment": "^2.29.0",
    "react": "^16.13.1",
    "react-beautiful-dnd": "^13.0.0",
    "react-dom": "^16.13.1",
    "react-router-dom": "^5.2.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/electron/electron-quick-start.git"
  },
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial",
    "demo"
  ],
  "author": "GitHub",
  "license": "CC0-1.0",
  "devDependencies": {
    "babel-plugin-import": "^1.13.3",
    "concurrently": "^5.3.0",
    "cross-env": "^7.0.3",
    "customize-cra": "^1.0.0",
    "less": "^3.13.0",
    "less-loader": "^7.1.0",
    "nodemon": "^2.0.4",
    "npm-run-all": "^4.1.5",
    "react-app-rewired": "^2.1.8",
    "react-scripts": "^4.0.1",
    "wait-on": "^5.2.0"
  },
  "bugs": {
    "url": "https://github.com/electron/electron-quick-start/issues"
  },
  "homepage": ".",
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
